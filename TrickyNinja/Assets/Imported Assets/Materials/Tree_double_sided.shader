            }
            fixed4 frag(VertexOutput i) : COLOR {
                float2 node_14 = i.uv0;
                float4 node_2 = tex2D(_Diffuse,TRANSFORM_TEX(node_14.rg, _Diffuse));
                clip(node_2.a - 0.5);
                SHADOW_CASTER_FRAGMENT(i)
            }
            ENDCG
        }
    }
